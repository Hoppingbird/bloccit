<h1>All Questions</h1>
<% @questions.each do |question| %>
     <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to question.title, question %>
       </h4>
       <h5><%= link_to question.body, question %></h5>
       <h7> Resolved: <%= "#{question.resolved}" %> </h7>
     </div>
   </div>
<% end %>